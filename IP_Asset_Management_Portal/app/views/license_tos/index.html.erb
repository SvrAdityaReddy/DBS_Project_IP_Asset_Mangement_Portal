<p id="notice"><%= notice %></p>

<h1>License Tos</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Uid</th>
      <th>License agreement</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @license_tos.each do |license_to| %>
      <tr>
        <td><%= license_to.name %></td>
        <td><%= license_to.uid %></td>
        <td><%= license_to.licenceagreement %></td>
        <td><%= link_to 'Show', license_to %></td>
        <td><%= link_to 'Edit', edit_license_to_path(license_to) %></td>
        <td><%= link_to 'Destroy', license_to, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New License To', new_license_to_path %>
